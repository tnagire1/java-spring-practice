package com.teja.exceptions;

public class ExceptionHandlingRunner {

	public static void main(String[] args) {

		method1();
		System.out.println("Main method ended.");

	}

	static void method1() {
		method2();
		System.out.println("Method1 Ended");

	}

	static void method2() {
		String str = null;
		str.length();
		System.out.println("Method1 Ended");

	}

}
